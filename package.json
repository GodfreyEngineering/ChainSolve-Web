{
  "name": "chainsolve-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run wasm:build && tsc -b && vite build",
    "wasm:build": "wasm-pack build crates/engine-wasm --target web --release",
    "wasm:build:dev": "wasm-pack build crates/engine-wasm --target web --dev",
    "wasm:test": "cargo test --workspace",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "typecheck": "npm run wasm:build && tsc -b --noEmit",
    "typecheck:functions": "tsc -p functions/tsconfig.json --noEmit",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "bench:rust": "cargo bench --package engine-core"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.97.0",
    "@xyflow/react": "^12.10.1",
    "i18next": "^25.8.13",
    "i18next-browser-languagedetector": "^8.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^7.13.1",
    "stripe": "^20.3.1",
    "vega": "^6.2.0",
    "vega-interpreter": "^2.2.1",
    "vega-lite": "^6.4.2",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260303.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1"
  }
}
